{
  "account": "near-merch-store.near",
  "app": {
    "host": {
      "title": "Near Merch",
      "development": "http://localhost:3001",
      "production": "https://near.everything.market"
    },
    "ui": {
      "name": "marketplace_ui",
      "development": "http://localhost:3002",
      "production": "https://elliot-braem-1160-marketplace-ui-near-merch-store-d1f23983d-ze.zephyrcloud.app",
      "exposes": {
        "App": "./App",
        "components": "./components",
        "providers": "./providers",
        "types": "./types"
      }
    },
    "api": {
      "plugins": {
        "marketplace-api": {
          "development": "http://localhost:3014",
          "production": "https://elliot-braem-1159-marketplace-api-near-merch-stor-b25e2cda2-ze.zephyrcloud.app",
          "variables": {
            "network": "mainnet",
            "contractId": "social.near"
